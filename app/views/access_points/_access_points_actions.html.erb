<% if @favourite %>
<span class="reset-favourites">
    <a href="<%= wisp_reset_favourites_path(@wisp)%>" class="hastip" title="<%= t :Remove_all_from_favourites %>"><%= t :Reset_all %></a>
</span>
<% end %>
<span class="hastip" title="<%= t :Select_all_title %>">
    <input type="checkbox" class="select-all-checkbox">
    <a class="select-all" href="#select-all"><%= t :Select_all %></a>
</span>
<a class="batch-change-group hastip" href="<%= select_group_url_if_wisp_loaded %>" data-fail-message="<%= t :No_access_point_selected %>" title="<%= t :Change_group_title %>">
    <%= t :Change_group %>
</a>